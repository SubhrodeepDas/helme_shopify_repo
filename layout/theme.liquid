<!doctype html>
<html>
  <head>
    <title>{{ page_title }}</title>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="description" content="{{ page_description | escape }}">
    <link rel="canonical" href="{{ canonical_url }}">
    <meta name="viewport" content="width=device-width,initial-scale=1,shrink-to-fit=no">
    {{ content_for_header }}
    <!-- Header hook for plugins -->
    <link href="https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,100;0,300;0,400;0,500;0,700;0,900;1,100;1,300;1,400;1,500;1,700;1,900&display=swap" rel="stylesheet">
    {{ 'bootstrap.min.css' | asset_url | stylesheet_tag }}
    <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css" rel="stylesheet">
    {{ 'meanmenu.css' | asset_url | stylesheet_tag }}
    {{ 'main.css' | asset_url | stylesheet_tag }}
    {{ 'responsive.css' | asset_url | stylesheet_tag }}
  </head>
  <body>

    {% section 'header' %}

    <main role="main">
      {{ content_for_layout }}
    </main>

    {% section 'footer' %}


    <script src="{{ 'jquery-3.7.1.min.js' | asset_url }}" async></script>
    <script src="{{ 'bootstrap.bundle.min.js' | asset_url }}" async></script>
    <script src="{{ 'jquery.meanmenu.min.js' | asset_url }}" async></script>
    <script src="{{ 'wow.min.js' | asset_url }}" async></script>
    <script src="{{ 'main.js' | asset_url }}" async></script>


  </body>
</html>